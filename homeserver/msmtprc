defaults
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.msmtp.log

account default
from hanavi@gmail.com
user hanavi@gmail.com
passwordeval    "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mutt/msmtp.gpg"
host smtp.gmail.com
port 587
protocol smtp
auth on
